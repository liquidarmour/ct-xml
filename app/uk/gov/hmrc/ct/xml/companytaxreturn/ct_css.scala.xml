* {
    position: relative;
    z-index: 2;
}

.draft .ct600-panel {
    z-index: -1;
}

.draft .page-break,
.draft #AccountsHeader,
.draft .accounts-comps-ct {
    zoom: expression(this.runtimeStyle.zoom="1", this.insertBefore(document.createElement("i"), this.firstChild).className="ie-after");
}

.draft .page-break:after,
.draft #AccountsHeader:after,
.draft .accounts-comps-ct:after {
    content: "Draft";
    color: #888;
    display: block;
    font-size: 128px;
    left: 50%;
    line-height: 1em;
    margin: -64px 0 0 -210px;
    opacity: 0.5;
    position: absolute;
    text-transform: uppercase;
    top: 50%;
    -moz-transform: rotate(-30deg);
    -webkit-transform: rotate(-30deg);
    -o-transform: rotate(-30deg);
    -ms-transform: rotate(-30deg);
    transform: rotate(-30deg);
    z-index: 5;
}

.draft .page-break .ie-after,
.draft #AccountsHeader .ie-after,
.draft .accounts-comps-ct .ie-after {
    color: #888;
    display: block;
    filter: alpha(opacity=50);
    font-size: 128px;
    left: 50%;
    line-height: 1em;
    margin: -64px 0 0 -210px;
    position: absolute;
    text-transform: uppercase;
    top: 50%;
    z-index: 5;
    zoom: expression(this.runtimeStyle.zoom="1", this.innerHTML = "DRAFT");
}

.draft #ct600 .page-break:after {
    display: none;
}

.negative,
.debit {
    white-space: nowrap;
}