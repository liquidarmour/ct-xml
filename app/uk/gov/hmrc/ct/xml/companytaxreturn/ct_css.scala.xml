/*** TYPOGRAPHY ***/
h1,
.heading-large {
    font-size: 2.124em;
    line-height: 1.625em;
    font-weight: 700;
    text-transform: none;

}

h2,
.heading-medium {
    font-size: 1.618em;
    line-height: 1.625em;
    text-transform: none;
    font-weight: 700;
}

h2.heading-small span {
    display: block;
}

h3,
.heading-small {
    font-size: 1.313em;
    line-height: 1.625em;
    text-transform: none;
    font-weight: 700;
}

h3.heading-small {
    background-color: #2b8cc4;
    color: #fff;
    display: inline-block;
    padding: 0.25em 0.5em;
    margin-bottom: 0;
}

p,
h1,
h2,
h3,
h4 {
    margin: 0 0 1.625em 0;
}

pre,
code {
    color: #0b0c0c;
    background-color: #f8f8f8;
    border: 1pt solid #bfc1c3;
    padding: 0.231em 0.538em;
}

.comps-body, .accounts-body {
    font-family: 'Times New Roman';
}

.margin-large {
    margin-top: 0.308em;
}

.margin-xx-large {
    margin-top: 10em;
}

.ct-body {
    font-size: 13px;
}

/*** LAYOUTS ***/

.comps-body, .ct-body, .page-break, .page-break-only {
  page-break-before:always;
}

@@media screen
{
    .comps-body, .accounts-body, .ct-body, .page-break, .page-break-only {
        margin-top: 20em;
    }
}

.no-page-break {
  page-break-inside: avoid;
}

.ct600a-company-info {
      margin-top: 0.5em!important;
}
details {
    clear: both;
}
.page-number {
    display: block;
    text-align: right;
}
.header {
    padding: 0.385em;
}


/*** GRID ***/

  .inner-block {
      margin-left: 1.154em;
      margin-right: 1.154em;
  }
  .grid-wrapper:after {
      content: '';
      display: block;
      clear: both;
  }
  @@media all {
      .grid-wrapper {
          margin-left: -1.154em;
          margin-right: -1.154em;
      }
  }
  .grid-divider {
      border-bottom: 1pt solid #bfc1c3;
      margin-bottom: 1.538em;
      margin-left: 1.154em;
      margin-right: 1.154em;
  }
  .grid-divider:after {
      content: "";
      display: block;
      clear: both;
  }
  @@media all {
      .grid-divider {
          margin-left: 2.308em;
          margin-right: 2.308em;
      }
  }
  .grid {
      float: left;
      width: 100%;
  }
  @@media all {
      .grid-1-4 {
          width: 25%;
      }
  }
  @@media all {
      .grid-1-3 {
          width: 33.33333%;
      }
  }
  @@media all {
      .grid-1-2 {
          width: 50%;
      }
  }
  @@media all {
      .grid-2-3 {
          width: 66.66667%;
      }
  }
  @@media all {
      .grid-3-4 {
          width: 75%;
      }
  }
  .grid .inner-block {
      padding: 0;
  }
  @@media all {
      .grid .inner-block {
          margin: 0 1.154em;
      }
  }
  .inner-block .grid-wrapper {
      padding: 0;
      margin-left: -1.154em;
      margin-right: -1.154em;
  }
  .inner-block .grid-divider {
      margin-left: 0;
      margin-right: 0;
  }

/*** TABLE ***/
table {
    border-collapse: collapse;
    font-size: 1em;
    font-family: Times New Roman;
    width: 100%;
    padding-bottom: 1.154em;
}
th {
    font-weight: bold;
    vertical-align: top;
    text-align: left;
}
td {
    padding: 0.8125em 0;
    text-align: left;
    vertical-align: top;
}

.table-heading {
    text-align: right;
    font-style: italic
}

td.figure,
td.total,
td.total-no-top-border,
td.grandtotal,
td.total-no-top-border,
td.grandtotal-no-top-border,
th.figure {
    vertical-align: middle;
    text-align: right;
    padding: 0.813em 0 0.813em 0.406em;
    white-space: nowrap;
    width: 10%;
}

@@media print {
    td.figure,
    td.total,
    td.total-no-top-border,
    td.grandtotal,
    td.total-no-top-border,
    td.grandtotal-no-top-border,
    th.figure {
        width: 15%;
    }
}

td.description {
    width: 50%;
}
td.total {
    border-top: black solid 1pt;
    border-bottom: black solid 1pt;
}
td.total-no-top-border {
    border-bottom: black solid 1pt;
}
td.grandtotal-no-top-border {
    border-bottom: black double 5pt;
}
td.grandtotal {
    border-top: black solid 1pt;
    border-bottom: black double 5pt;
}
td.align-right {
    text-align: right;
}

.ct600a-table {
    border-top: 1pt solid black;
    padding: 0;
}
.ct600a-table td {
    border-bottom: 1pt solid black;
    border-left: 1pt solid black;
    border-right: 1pt solid black;
    background: white;
    padding: 0 0.231em;
}
.ct600a-table td.clear {
    background: transparent;
    border: 0;
}
.ct600a-table td.last {
    border-left: 0;
}



/*** PANEL ***/
.ct600-panel {
  background-color: #d5e8f3;
  padding: 0.769em;
  margin-bottom: 1em;
}
.ct600-panel:after {
  content: '';
  display: block;
  clear: both;
}


/** FIELD LEVEL ***/
.field-number {
  margin-right: 0.25em;
  font-weight: 700;
}

.calculation-notes {
  background-color: #2b8cc4;
  color: #fff;
  display: block;
  text-align: center;
  margin: 0.385em 0 -0.385em -0.077em;
  font-size: 0.811em;
  padding: 0.4em 0;
}

.calculation-notes.calculation-notes-left {
  margin-left: 0;
}

.form-hint {
  display: inline;
}



.ct600-output {
  display: block;
  background-color: #fff;
  border: 1pt solid #bfc1c3;
  padding: 0.25em 0.5em;
  margin: 0.25em 0 0.35em;
}
.ct600-output:after {
  content: '';
  display: block;
  clear: both;
}
.ct600-output-inline {
    display: inline;
    margin: 0 0.25em 0 0.5em;
}
.ct600-output-large {
  height: 4em;
}
.ct600-output-medium {
  width: 9.8em;
}
.ct600-output-small {
  width: 5em;
}
.ct600-output-small-inline {
  width: 2em;
  display: inline-block;
  margin: 0 0.5em -0.1em 0;
}


.dob {
    margin-right: 0.231em;
    display: inline-block;
    float: left;
    border: 0;
}
.dob .ct600-output-small-inline {
  float: left;
  width: 0.5em;
}
.ct600-output-small-inline.day,
.ct600-output-small-inline.month {
    border-right: 1pt solid #000000;
}

.ct600-output.ct600-output-with-calc-notes {
    padding: 0;
}
.ct600-output-with-calc-notes .calculation-notes {
    margin: 0;
}
.ct600-output-with-calc-notes .box-numbers {
    margin-top: 0;
    margin-bottom: 0;
}



.box-numbers {
    background-color: #2b8cc4;
    color: #fff;
    margin: -0.5em 0.5em -0.5em -0.6em;
    width: 2.5em;
    padding: 0.25em 0;
    text-align: center;
    display: inline-block;
    border: 1pt solid #2b8cc4;
}
.box-numbers-light {
    background-color: #96c6e2;
    color: #005ea5;
}


/** LISTS **/
details,
summary {
    display: block;
}
menu,
ol,
ul {
    margin-top: 0.385em;
    margin-bottom: 1.538em;
    padding: 0;
}
ul,
ol {
    list-style-type: none;
}
ul li {
    margin-bottom: 0.385em;
