/*** TYPOGRAPHY ***/
h1 {
    font-weight: bold;
    font-size: 18px;
}
.ct-body h1 {
    font-size: 14px;
}

h2 {
    font-weight: bold;
    font-size: 16px;
}
h3 {
    font-weight: bold;
    font-size: 14px;
}

.heading-large {
    font-size: 24px;
    line-height: 1.11111;
    font-weight: 400;
    text-transform: none;
    font-weight: 700;
    margin-bottom: 0.41667em;
}

h3.heading-small {
    background-color: #2b8cc4;
    color: #fff;
    display: inline-block;
    padding: 0.25em 0.5em;
    margin-bottom: 0;
}
h2.heading-small span {
    display: block;
}

.heading-medium {
    font-size: 19px;
    line-height: 1.25;
    font-weight: 400;
    text-transform: none;
    font-weight: 700;
    margin-bottom: 0.5em;
}

.heading-small {
    font-size: 14px;
    line-height: 1.31579;
    font-weight: 400;
    text-transform: none;
    font-weight: 700;
    margin-bottom: 0.3125em;
}

p {
    margin-top: 0.3125em;
    margin-bottom: 1.25em;
}
p.accounts-upload {
    font-size: 1.3em;
}

pre,
code {
    font-size: 13px;
    line-height: 19px;
    color: #0b0c0c;
    background-color: #f8f8f8;
    border: 1px solid #bfc1c3;
    padding: 3px 7px;
}

.comps-body, .accounts-body {
    font-family: 'Times New Roman';
}

.margin-large {
    margin-top: 4em;
}

.margin-xx-large {
    margin-top: 10em;
}

/*** LAYOUTS ***/

.comps-body, .ct-body {
  page-break-before:always;
}
details {
    clear: both;
}
.page-number {
    display: block;
    text-align: right;
}
.header {
    padding: 5px;
}


/*** GRID ***/

.inner-block {
    margin-left: 15px;
    margin-right: 15px;
}
.grid-wrapper:after {
    content: '';
    display: block;
    clear: both;
}
@@media all {
    .grid-wrapper {
        margin-left: -15px;
        margin-right: -15px;
    }
}
.grid-divider {
    border-bottom: 1px solid #bfc1c3;
    margin-bottom: 20px;
    margin-left: 15px;
    margin-right: 15px;
}
.grid-divider:after {
    content: "";
    display: block;
    clear: both;
}
@@media all {
    .grid-divider {
        margin-left: 30px;
        margin-right: 30px;
    }
}
.grid {
    float: left;
    width: 100%;
}
@@media all {
    .grid-1-4 {
        width: 25%;
    }
}
@@media all {
    .grid-1-3 {
        width: 33.33333%;
    }
}
@@media all {
    .grid-1-2 {
        width: 50%;
    }
}
@@media all {
    .grid-2-3 {
        width: 66.66667%;
    }
}
@@media all {
    .grid-3-4 {
        width: 75%;
    }
}
.grid .inner-block {
    padding: 0;
}
@@media all {
    .grid .inner-block {
        margin: 0 15px;
    }
}
.inner-block .grid-wrapper {
    padding: 0;
    margin-left: -15px;
    margin-right: -15px;
}
.inner-block .grid-divider {
    margin-left: 0;
    margin-right: 0;
}

/*** TABLE ***/
table {
    font-family: Times New Roman;
    width: 100%;
    padding-bottom: 15px;
}
th {
    font-weight: bold;
    vertical-align: top;
    text-align: left;
}
td {
    vertical-align: top;
    text-align: left;
}

.table-heading {
    text-align: right;
    font-style: italic
}

td.figure,
td.total,
td.total-no-top-border,
td.grandtotal,
td.total-no-top-border,
td.grandtotal-no-top-border,
th.figure {
    vertical-align: middle;
    text-align: right;
    padding: 2px 5px;
    white-space: nowrap;
    width: 10%;
}
td.description {
    width: 50%;
}
td.total {
    border-top: black solid 1px;
    border-bottom: black solid 1px;
}
td.total-no-top-border {
    border-bottom: black solid 1px;
}
td.grandtotal-no-top-border {
    border-bottom: black double 5px;
}
td.grandtotal {
    border-top: black solid 1px;
    border-bottom: black double 5px;
}
td.align-right {
    text-align: right;
}

table.wrap th,
table.wrap td {
    white-space: normal;
}

@@media print {
    td.figure,
    td.total,
    td.total-no-top-border,
    td.grandtotal,
    td.total-no-top-border,
    td.grandtotal-no-top-border,
    th.figure {
        width: 15%;
    }
}

/** FIELD LEVEL ***/
.field-number {
  margin-right: 0.25em;
  font-weight: 700;
}

/** LISTS **/
details,
summary {
    display: block;
}
menu,
ol,
ul {
    margin-top: 5px;
    margin-bottom: 20px;
    padding: 0;
}
ul,
ol {
    list-style-type: none;
}

ul.with-list-style {
    font-size: 1.3em;
    list-style: disc;
    margin-left: 3em;
}

ul li {
    margin-bottom: 5px;
}
.list-bullet {
    list-style-type: disc;
    margin-left: 20px;
}
.list--dashed {
    padding-left: 1.5em;
}
.list--dashed li:before {
    content: '-';
    display: inline-block;
    margin-left: -1.5em;
    width: 1.5em;
}

.logo {
  display: block;
}

ul.accounts-comps-ct {
   margin-top: 240px;
   text-align:center;
   font-size:24px;
}

.page-break {
    min-height: 400px;
    page-break-before:always;
}

.iris_hr {
    page-break-after: always;
}

.page-break-only {
    page-break-before: always;
}

.no-page-break {
  page-break-inside: avoid;
}

.page {
    height: 261mm;
    margin: 0 auto;
    page-break-after: always;
    position: relative;
    width: 185mm;
}

.scaled .page {
    height: 230mm;
    width: 160mm;
}

@@page {
    size: A4;
}

@@media print {
    html, body {
        height: 261mm;
        width: 185mm;
    }
}

@@media screen {
    .page {
        margin-bottom: 5em
    }
}

.section-marker {
    left: 0;
    position: absolute;
    top: 0;
}

.page-footer {
    box-sizing: border-box;
    height: 100%;
    left: 0;
    padding-top: 135%;
    pointer-events: none;
    position: absolute;
    top: 0;
    width: 100%;
}

.draft .page-break,
.draft #AccountsHeader {
    zoom: expression(this.runtimeStyle.zoom="1", this.insertBefore(document.createElement("i"), this.firstChild).className="ie-after");
}

.draft .page:after,
.draft .page-break:after,
.draft #AccountsHeader:after {
    content: "Draft";
    color: #888;
    display: block;
    font-size: 128px;
    left: 50%;
    line-height: 1em;
    margin: -64px 0 0 -210px;
    opacity: 0.5;
    position: absolute;
    text-transform: uppercase;
    top: 50%;
    -moz-transform: rotate(-30deg);
    -webkit-transform: rotate(-30deg);
    -o-transform: rotate(-30deg);
    -ms-transform: rotate(-30deg);
    transform: rotate(-30deg);
    z-index: 5;
}

.draft .page .ie-after,
.draft .page-break .ie-after,
.draft #AccountsHeader .ie-after {
    color: #888;
    display: block;
    filter: alpha(opacity=50);
    font-size: 128px;
    left: 50%;
    line-height: 1em;
    margin: -64px 0 0 -210px;
    position: absolute;
    text-transform: uppercase;
    top: 50%;
    z-index: 5;
    zoom: expression(this.runtimeStyle.zoom="1", this.innerHTML = "DRAFT");
}

.split-equally tr td,
.split-equally tr td.figure,
.split-equally tr td.total {
    width: 50%;
}

.negative,
.debit {
    white-space: nowrap;
}